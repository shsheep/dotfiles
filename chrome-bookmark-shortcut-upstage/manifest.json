{
    "name": "Chrome Bookmark Shortcut Upstage",
    "version": "1.0",
    "description": "Custom shortcuts for bookmarks!",

    "permissions": ["tabs"],
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["background.js"],
        "run_at": "document_start"
      }
      ],
    "manifest_version": 3
  }
